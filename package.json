{
  "name": "vota-operator-scripts",
  "version": "1.0.0",
  "description": "vota operator scripts",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18"
  },
  "exports": {
    ".": "./dist/index.js"
  },
  "type": "commonjs",
  "types": "src/index.ts",
  "bin": {
    "operator": "./dist/operator.mjs"
  },
  "scripts": {
    "build": "pnpm run build:js",
    "build:js": "tsup && chmod +x ./dist/operator.mjs",
    "clean": "pnpm run clean:js",
    "clean:js": "rimraf dist",
    "dev": "tsup --watch",
    "lint": "eslint . --ext .ts",
    "genOperatorKey": "node js/genOperatorKey.js",
    "operator": "node ./dist/operator.mjs"
  },
  "license": "ISC",
  "dependencies": {
    "@cosmjs/cosmwasm-stargate": "^0.31.0",
    "@cosmjs/launchpad": "^0.27.1",
    "@cosmjs/proto-signing": "^0.31.0",
    "@cosmjs/stargate": "^0.31.0",
    "chalk": "^5.0.1",
    "chokidar": "^3.5.3",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.8",
    "execa": "^7.0.0",
    "find-up": "^6.3.0",
    "glob": "^8.0.3",
    "nice-grpc-web": "^2.0.1",
    "openurl": "^1.1.1",
    "path": "^0.12.7",
    "throttle-debounce": "^5.0.0",
    "typechain": "^8.1.1",
    "typescript": "5.1.6",
    "yargs": "^17.7.1",
    "zod": "^3.21.4",
    "prettier": "^2.8.4",
    "prettier-plugin-rust": "^0.1.9",
    "zod-validation-error": "^1.3.0",
    "esbuild": "^0.17.15",
    "tsup": "^6.7.0",
    "tsx": "^3.12.6",
    "vitest": "0.31.4",
    "ts-node": "^10.9.1",
    "blake-hash": "^2.0.0",
    "circom": "https://github.com/weijiekoh/circomlib#24ed08eee0bb613b8c0135d66c1013bd9f78d50a",
    "circomlib": "2.0.0",
    "csv": "^6.0.5",
    "ethers": "^5.5.2",
    "ffjavascript": "^0.2.46",
    "web3": "^1.6.1"
  }
}
